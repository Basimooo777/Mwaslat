<body onload="showingNodes()" >
	<div id="map_canvas" style="width:100%; height:100%"></div>
	<div class="container">
		<div class="row">
			<div class="span9">
				<p>
					<%= notice %>
				</p>
				<%= form_for @node , :html => { :class => 'form-horizontal' } do |f| %>
				<h2 >New Node</h2>
				<hr/>
				<%= render 'routes/node_fields',:f => f %>
				 <div class="form-actions">
				 	<%= f.submit('Save', :onclick => "fillNodePath();",:class =>'btn btn-small btn-primary') %>
					<%= link_to 'Cancel', nodes_path, :class => 'btn' %>
				</div>
				<% end %>
			</div>
		</div>
	</div>
</body>