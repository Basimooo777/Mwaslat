<div class="container-fluid" style="height: 100%;">
	<div class="row-fluid" id="hasmap" style="height: 100%; " >
		<p id="notice">
			<%= notice %>
		</p>
		<h2>All Routes</h2>
		<table id="users_table" class="table table-striped table-bordered" >
			<thead>
				<tr>
					<th>Id</th>
					<th>Email</th>
				</tr>
			</thead>
			<tbody>
				<% @users.each do |user| %>
				<tr>
					<td><%= user.id %></td>
					<td><%= link_to user.email, user %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
		<%= will_paginate @users%>
	</div>
</div>